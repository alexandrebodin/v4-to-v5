{
  "name": "@v4-to-v5/plugin-basic-ts",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi plugin",
  "scripts": {
    "build": "pnpm run build:admin && pnpm run build:server",
    "build:server": "tsc -p tsconfig.server.json",
    "build:admin": "tsc -p tsconfig.admin.json"
  },
  "devDependencies": {
    "@strapi/strapi": "5.0.0-rc.19",
    "@types/react": "^18",
    "react": "^18.0.0",
    "typescript": "^5"
  },
  "strapi": {
    "kind": "plugin",
    "name": "basic-ts",
    "displayName": "Basic - TypeScript",
    "description": "A basic TypeScript plugin"
  }
}
